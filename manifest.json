{
    "name": "Friendica",
    "id": "friendica",
    "packaging_format": 1,
    "description": {
        "en": "Social Communication Server",
        "fr": "Serveur de Communication Social"
    },
    "version": "2022.12~ynh1",
    "url": "http://friendi.ca",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "http://friendi.ca",
        "demo": "https://dir.friendica.social/servers",
        "admindoc": "https://github.com/friendica/friendica/wiki",
        "userdoc": "https://wiki.friendi.ca/",
        "code": "https://github.com/friendica/friendica",
        "cpe": "cpe:2.3:a:friendica:friendica"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Anmol Sharma",
        "email": ""
    },
    "previous_maintainers": {
        "name": "aymhce",
        "email": "aymhce@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "language",
                "type": "string",
                "ask": {
                    "en": "Choose the application language",
                    "fr": "Choisissez la langue de l'application"
                },
                "choices": [
                    "de",
                    "en",
                    "es",
                    "fr",
                    "it",
                    "pt"
                ],
                "default": "en"
            }
        ]
    }
}